# Google Analytics Measurement ID (GA4)
# Get this from your Google Analytics account (Admin → Data Streams → Web)
NEXT_PUBLIC_GA_MEASUREMENT_ID="G-XXXXXXXXXX"

# Google Tag Manager ID
# Get this from your Google Tag Manager account
NEXT_PUBLIC_GTM_ID="GTM-XXXXXXXX"

# Microsoft Clarity Project ID
# Get this from your Microsoft Clarity account (Dashboard → Settings)
NEXT_PUBLIC_CLARITY_PROJECT_ID="XXXXXXXXXX"

# Site URL for metadata
NEXT_PUBLIC_SITE_URL=https://example.com

# Compression and Performance Settings
ANALYZE=false
NODE_ENV=production